{
  "root": true,
  "env": {
    "browser": true,
    "node": true,
    "es2021": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended",
    "plugin:react/recommended",
    "plugin:react/jsx-runtime",
    "react-app",
    "react-app/jest"
  ],
  "plugins": [],
  "rules": {
    "no-unused-vars": "off",
    "no-undef": "off",
    "react/prop-types": [0],
    "react-hooks/exhaustive-deps": [0]
  }
}
